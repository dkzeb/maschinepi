{
  "name": "maschinepi",
  "version": "0.1.0-alpha",
  "description": "A Maschine MK3 Standalone App",
  "scripts": {
    "build": "npm run build-code && npm run build-bundle",
    "postbuild": "node install-prebuilds.js",
    "build-code": "tsc",
    "postinstall": "patch-package",
    "build-bundle": "esbuild build/main.js --sourcemap --bundle --outfile=dist/main.cjs --platform=node --format=cjs --external:node-web-audio-api --external:canvas --external:@nodegui/nodegui \"--external:nodegui-plugin-*\"",
    "clean": "rimraf build dist tmp-jam-pack-nodegui",
    "package": "jam-pack-nodegui --config packaging/jam-pack-nodegui.json",
    "run": "qode --force-node-api-uncaught-exceptions-policy=true dist/main.cjs",
    "start": "npm run build && npm run run",
    "ts": "ts-node"
  },
  "author": "zeb",
  "license": "MIT",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.10",
    "@types/node": "^20.3.1",
    "@types/node-wav": "^0.0.3",
    "esbuild": "^0.18.4",
    "jam-pack-nodegui": "^0.18.0",
    "patch-package": "^8.0.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@canvas-fonts/impact": "^1.0.4",
    "@elemaudio/core": "^3.2.1",
    "@elemaudio/offline-renderer": "^3.2.2",
    "@nodegui/nodegui": "^0.61.0",
    "@prisma/client": "^5.14.0",
    "better-sqlite3": "^10.0.0",
    "canvas": "^2.11.2",
    "jpeg-js": "^0.4.4",
    "ni-controllers-lib": "^1.2.3",
    "node-hid": "^3.0.0",
    "node-wav": "^0.0.2",
    "node-web-audio-api": "^0.21.0",
    "prisma": "^5.14.0",
    "rxjs": "^7.8.1",
    "smart-buffer": "^4.2.0",
    "source-map-support": "^0.5.21",
    "speaker": "^0.5.5",
    "usb": "^2.12.1",
    "wavefile": "^11.0.0"
  }
}
