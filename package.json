{
  "name": "maschinepi",
  "version": "1.0.0",
  "description": "A Maschine MK3 Standalone App",
  "scripts": {
    "build": "npm run build-code && npm run build-bundle",
    "postbuild": "cp -r node_modules/node-hid/prebuilds/. ./dist/prebuilds/",
    "build-code": "tsc",
    "build-bundle": "esbuild build/main.js --sourcemap --bundle --outfile=dist/main.cjs --platform=node --format=cjs --external:@nodegui/nodegui \"--external:nodegui-plugin-*\"",
    "clean": "rimraf build dist tmp-jam-pack-nodegui",
    "package": "jam-pack-nodegui --config packaging/jam-pack-nodegui.json",
    "run": "qode --force-node-api-uncaught-exceptions-policy=true dist/main.cjs",
    "start": "npm run build && npm run run",
    "ts": "ts-node"
  },
  "author": "zeb",
  "license": "MIT",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.10",
    "@types/node": "^20.3.1",
    "@types/node-wav": "^0.0.3",
    "esbuild": "^0.18.4",
    "jam-pack-nodegui": "^0.18.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@nodegui/nodegui": "^0.61.0",
    "@prisma/client": "^5.14.0",
    "audify": "^1.9.0",
    "audio-buffer": "^5.0.0",
    "better-sqlite3": "^10.0.0",
    "naudiodon2": "^2.4.0",
    "node-hid": "^3.0.0",
    "node-wav": "^0.0.2",
    "prisma": "^5.14.0",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "usb": "^2.12.1",
    "wav-blob-util": "^1.0.21",
    "wav-file-encoder": "^1.0.4",
    "wavefile": "^11.0.0"
  }
}
